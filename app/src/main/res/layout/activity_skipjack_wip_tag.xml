<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_skipjack_wip_tag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SkipjackWipTagActivity">

    <TextView
        android:id="@+id/view_shift"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Cyan_050"
        android:ems="10"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="dd.MM.yyyy - DN"
        android:inputType="text"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/text_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:background="@color/Red_A700"
        android:fontFamily="serif"
        android:hint="#USER"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:textColor="@color/White"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/view_shift"
        app:layout_constraintTop_toTopOf="@+id/view_shift" />

    <TextView
        android:id="@+id/btn_new"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:background="@color/Cyan_A400"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="NEW"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_each"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/text_each"
        app:layout_constraintTop_toTopOf="@+id/goto_edit" />

    <TextView
        android:id="@+id/btn_save"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:background="@color/Light_Green_A400"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="SAVE"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_rackNo"
        app:layout_constraintStart_toStartOf="@+id/text_rackNo"
        app:layout_constraintTop_toBottomOf="@+id/text_rackNo" />

    <TextView
        android:id="@+id/goto_edit"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:background="@color/Amber_A400"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="EDIT"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/text_tray"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/text_tray"
        app:layout_constraintTop_toTopOf="@+id/btn_save" />

    <EditText
        android:id="@+id/text_each"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:hint="#EACH"
        android:inputType="number"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/view_color3"
        app:layout_constraintStart_toStartOf="@+id/cap_frac"
        app:layout_constraintTop_toBottomOf="@+id/cap_frac"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/text_tray"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:hint="#TRAY"
        android:inputType="number"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/view_color2"
        app:layout_constraintStart_toStartOf="@+id/cap_tray"
        app:layout_constraintTop_toBottomOf="@+id/cap_tray"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/cap_frac"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="EACH"
        app:layout_constraintStart_toStartOf="@+id/view_color3"
        app:layout_constraintTop_toTopOf="@+id/cap_tray" />

    <TextView
        android:id="@+id/cap_tray"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="TRAY"
        app:layout_constraintStart_toStartOf="@+id/view_color2"
        app:layout_constraintTop_toTopOf="@+id/cap_rackNo" />

    <EditText
        android:id="@+id/text_rackNo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:hint="#RACK"
        android:inputType="number"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/view_color1"
        app:layout_constraintStart_toStartOf="@+id/cap_rackNo"
        app:layout_constraintTop_toBottomOf="@+id/cap_rackNo"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/cap_rackNo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="serif"
        android:text="RACK"
        app:layout_constraintStart_toStartOf="@+id/view_color1"
        app:layout_constraintTop_toBottomOf="@+id/view_color1" />

    <TextView
        android:id="@+id/view_color2"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@color/Blue_Gray_050"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="#COL2"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/view_color1"
        app:layout_constraintEnd_toEndOf="@+id/view_origin"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/view_origin"
        app:layout_constraintTop_toTopOf="@+id/view_color1"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/view_color3"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@color/Blue_Gray_050"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="#COL3"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/view_color2"
        app:layout_constraintEnd_toEndOf="@+id/text_size"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/text_size"
        app:layout_constraintTop_toTopOf="@+id/view_color2" />

    <TextView
        android:id="@+id/view_color1"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@color/Blue_Gray_050"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="#COL1"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="@+id/text_queueNo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_specy"
        app:layout_constraintTop_toBottomOf="@+id/view_specy" />

    <TextView
        android:id="@+id/view_specy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Amber_A100"
        android:fontFamily="serif"
        android:gravity="clip_horizontal"
        android:hint="#SPE"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="@+id/text_queueNo"
        app:layout_constraintStart_toStartOf="@+id/cap_specy"
        app:layout_constraintTop_toBottomOf="@+id/cap_specy" />


    <TextView
        android:id="@+id/view_origin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Amber_A100"
        android:fontFamily="serif"
        android:gravity="clip_horizontal"
        android:hint="#ORIGIN"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_specy"
        app:layout_constraintEnd_toEndOf="@+id/view_QueColor"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/cap_origin"
        app:layout_constraintTop_toBottomOf="@+id/cap_origin" />

    <TextView
        android:id="@+id/view_col"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="20dp"
        android:background="@color/Amber_A400"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="COL"
        android:textSize="34sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/view_QueColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view_QueColor"
        app:layout_constraintTop_toTopOf="@+id/view_QueColor"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/cap_specy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="serif"
        android:text="SPECIES"
        app:layout_constraintStart_toStartOf="@+id/text_queueNo"
        app:layout_constraintTop_toBottomOf="@+id/text_queueNo" />

    <TextView
        android:id="@+id/cap_origin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="ORIGIN"
        app:layout_constraintStart_toStartOf="@+id/view_QueColor"
        app:layout_constraintTop_toTopOf="@+id/cap_specy" />

    <TextView
        android:id="@+id/cap_size"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="SIZE"
        app:layout_constraintStart_toStartOf="@+id/view_col"
        app:layout_constraintTop_toTopOf="@+id/cap_origin" />

    <EditText
        android:id="@+id/text_size"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/Amber_A400"
        android:fontFamily="serif"
        android:hint="#SIZE"
        android:textSize="32sp"
        android:inputType="textCapCharacters"
        app:layout_constraintBottom_toBottomOf="@+id/view_origin"
        app:layout_constraintEnd_toEndOf="@+id/view_col"
        app:layout_constraintStart_toStartOf="@+id/view_col"
        app:layout_constraintTop_toBottomOf="@+id/cap_size" />

    <TextView
        android:id="@+id/view_QueColor"
        android:layout_width="110dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:background="@color/Amber_A400"
        android:fontFamily="serif"
        android:gravity="center"
        android:hint="[=]"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/text_queueNo"
        app:layout_constraintStart_toEndOf="@+id/text_queueNo"
        app:layout_constraintTop_toTopOf="@+id/text_queueNo" />

    <EditText
        android:id="@+id/text_queueNo"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:background="@color/Cyan_A200"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:gravity="center"
        android:hint="#QUE"
        android:inputType="number"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_shift"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="COOK TAG"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>