<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SkipjackCookMainActivity">

    <TextView
        android:id="@+id/goto_continue"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:background="@color/Light_Green"
        android:ems="10"
        android:fontFamily="serif"
        android:gravity="center"
        android:inputType="text"
        android:text="CONTINUE"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="@+id/goto_pickRackCount"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/capt_rackCount"
        app:layout_constraintTop_toBottomOf="@+id/text_rackCount"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/goto_pickRackCount"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:background="@color/Light_Green"
        android:ems="10"
        android:fontFamily="serif"
        android:gravity="center"
        android:inputType="text"
        android:text="[=]"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/text_rackCount"
        app:layout_constraintEnd_toEndOf="@+id/goto_pickSteam"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/text_steamNo"
        app:layout_constraintTop_toTopOf="@+id/text_rackCount"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/goto_pickSteam"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/Light_Green"
        android:ems="10"
        android:fontFamily="serif"
        android:gravity="center"
        android:inputType="text"
        android:text="[=]"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/text_steamNo"
        app:layout_constraintEnd_toEndOf="@+id/view_shift"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_shift"
        app:layout_constraintTop_toTopOf="@+id/text_steamNo"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/text_steamNo"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:gravity="center"
        android:hint="#"
        android:inputType="text"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView222"
        app:layout_constraintEnd_toEndOf="@+id/view_date"
        app:layout_constraintStart_toEndOf="@+id/textView222"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/capt_rackCount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="serif"
        android:gravity="right"
        android:text="RACK COUNT"
        app:layout_constraintEnd_toEndOf="@+id/textView222"
        app:layout_constraintStart_toStartOf="@+id/view_date"
        app:layout_constraintTop_toBottomOf="@+id/textView222" />

    <TextView
        android:id="@+id/goto_batchCook"
        android:layout_width="0dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="serif"
        android:gravity="center"
        android:inputType="text"
        android:text="BATCH COOK"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/goto_pickRackCount"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/capt_rackCount"
        app:layout_constraintTop_toBottomOf="@+id/goto_continue"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/text_rackCount"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="@color/Amber_A400"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:gravity="center"
        android:hint="#"
        android:inputType="text"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/capt_rackCount"
        app:layout_constraintEnd_toEndOf="@+id/text_steamNo"
        app:layout_constraintStart_toEndOf="@+id/textView222"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="COOK : #MAIN"
        android:textAlignment="center"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:fontFamily="serif"
        android:text="DATE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView222"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:fontFamily="serif"
        android:gravity="right"
        android:text="STEAM NO."
        app:layout_constraintStart_toStartOf="@+id/view_date"
        app:layout_constraintTop_toBottomOf="@+id/view_date" />

    <TextView
        android:id="@+id/view_date"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:background="@color/Cyan_050"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:gravity="center"
        android:hint="dd.MM.yyyy"
        android:inputType="text"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="serif"
        android:text="SHIFT"
        app:layout_constraintStart_toStartOf="@+id/view_shift"
        app:layout_constraintTop_toTopOf="@+id/textView22" />

    <TextView
        android:id="@+id/view_shift"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="32dp"
        android:background="@color/Cyan_050"
        android:ems="10"
        android:fontFamily="@font/roboto_mono"
        android:gravity="center"
        android:hint="DN"
        android:inputType="text"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view_date"
        app:layout_constraintTop_toTopOf="@+id/view_date"
        tools:ignore="TouchTargetSizeCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>